{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
    // "eslint:recommend",
    // "plugin:react/recommend"
  ],
  "plugins": ["import", "react", "react-hooks"],
  "rules": {
    "no-param-reassign": "off"
  }
}
